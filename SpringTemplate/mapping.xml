<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<class name="com.yulartech.templateDomain.Customers">

		<map-to xml="templateCustomers" />

		<field name="type" type="integer">
			<bind-xml name="type" node="attribute" />
		</field>

		<field name="action" type="string">
			<bind-xml name="action" node="element" />
		</field>

		<field name="Country" type="string">
			<bind-xml name="Country" node="element" />
		</field>
	</class>
</mapping>